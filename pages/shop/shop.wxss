/* pages/shop/shop.wxss */

page {
  min-height: 100%;
  background-color: #f5f5f9;
}

.vib{
  visibility: hidden;
}

.nav {
  width: 100%;
  font-size: 24rpx;
  background-color: #fff;
}

.nav .nav-list {
  width: 50%;
  line-height: 85rpx;
  display: inline-table;
  text-align: center;
  color: #e6a3a6;
  border-bottom: 4rpx solid #f9f9fb;
}

.nav .nav-list.z-index {
  color: #e4393c;
  border-color: #e4393c;
}

.authorization{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center
}
.authorization:after {
    content: "";
    display: inline-block;
    height: 100%;
    width: 0;
    vertical-align: middle;
}
.authorization .text{
  font-size: 26rpx;
  margin-top: 36rpx;
  color: #999999
}
.authorization>view{
  display: inline-block;
  vertical-align: middle;
}
.btn-authorization{
  width: 160rpx;
  height: 48rpx;
  line-height: 48rpx;
  color: #e4393c;
  background-color: transparent;
  font-size: 26rpx;
  border: 1rpx solid #e4393c;
  padding: 0
}

/***********************************/

.tip {
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  font-size: 25rpx;
  text-align: center;
  background-color: #fff;
  margin: 50rpx 0 10rpx 0;
  color: #666;
}

.main{
  width: 100%;
}

.main-list {
  width: 100%;
  padding: 28rpx 27rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f8f8fb;
  background-color: #fff;
  position: relative;
}

.main-list>.l {
  width: 161rpx;
  display: inline-block;
  vertical-align: top;
  position: relative;
}

.main-list>.l .photo {
  width: 160rpx;
  height: 160rpx;
  background-position: 50% 50%;
  background-size: cover;
  background-repeat: no-repeat;
  position: relative;
}

.main-list>.l.z-index .select {
  background-color: #e4393c;
}

.main-list>.l .select {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 0;
  left: 0;
  background-color: #bfbfbf;
  z-index: 9;
  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.main-list>.l .select image {
  width: 27rpx;
  height: 18rpx;
}

.main-list>.r {
  width:520rpx;
  font-size: 25rpx;
  display: inline-block;
  vertical-align: top;
  padding-left: 15rpx;
}
.main-list.edit>.r {
  width: 429rpx;
}
.main-list .num02{
  vertical-align: bottom;
  float:right;
  color:#87858f;
  margin-top:10rpx;
}
.main-list.edit .num02{
  display: none;
}


.main-list>.r .title {
  position: relative;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/*.main-list>.r .title text {
  width: 435rpx;
  display: inline-block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}*/

.main-list>.r .title image {
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  right:-10rpx;
  top:-7rpx;
  padding:15rpx;
}

.main-list>.r .specifications {
  color: #87858f;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}
.main-list>.remove{
  width: 108rpx;
  height: 100%;
  background-color: #e4393c;
  color: #fff;
  font-size: 22rpx;
  position: absolute;
  right: 0;
  top: 0;
  visibility: hidden;

  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.main-list .price>.l {
  width: 226rpx;
  display: inline-block;
  color: #e4393c;
}

.main-list .price>.l .rmb {
  font-size: 20rpx;
}

.main-list .price>.l .money {
  font-size: 26rpx;
}

.main-list .price>.l .moneySmall {
  font-size: 20rpx;
}

.main-list .price>.r {
  width: 200rpx;
  display: inline-block;
  visibility: hidden;
}

.main-list .price>.r view {
  width: 60rpx;
  height: 55rpx;
  line-height: 55rpx;
  background-color: #f3f2f8;
  text-align: center;
  margin-right: 2rpx;
  display: inline-block;
}

.main-list .price>.r view image {
  width: 20rpx;
  height: 20rpx;
}

.main-list .price>.r view.num {
  width: 72rpx;
}

.main-list .price>.r view.add {
  font-weight: bold;
}

.main-list.edit>.remove, .main-list.edit .price>.r{
  visibility: visible;
}
/********************************************/

.settlement {
  width: 100%;
  height: 99rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  font-size: 28rpx;
  z-index: 999;
}

.settlement>view {
  display: inline-block;
}

.settlement .select {
  width: 40rpx;
  height: 40rpx;
  background-color: #bfbfbf;
  margin: 0 auto;
  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.settlement .select.z-index {
  background-color: #e4393c;
}

.settlement .select image {
  width: 27rpx;
  height: 18rpx;
}

.settlement .box1, .settlement .box2, .settlement .box3 {
  margin-top: 28rpx;
}

.settlement .box1 {
  width: 92rpx;
}

.settlement .box2 {
  width: 126rpx;
  text-align: left;
}

.settlement .box3 {
  width: 328rpx;
}

.settlement .box3 .p {
  color: #e4393c;
}

.settlement .box4 {
  width: 200rpx;
  height: 99rpx;
  line-height: 99rpx;
  display: inline-block;
  text-align: center;
  background-color: #e4393c;
  position: absolute;
  right: 0;
  top: 0;
  color: #fff;
  font-size: 32rpx;
}

/****************************/

.shopName {
  width: 100%;
  box-sizing: border-box;
  padding: 16rpx 27rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  border-bottom: 1rpx solid #f5f5f9;
  background-color: #fff;
}

.shopName image {
  width: 47rpx;
  height: 40rpx;
  vertical-align: top;
  margin-right: 15rpx;
}

.shopName>view{
  float: right;
  color: #87858f;
}
/************************/
.clean{
  width: 176rpx;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  color: #f33030;
  border-radius: 5rpx;
  border: 2rpx solid #f33030;
  font-size: 25rpx;
  margin: 20rpx auto
}