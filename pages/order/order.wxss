/* pages/order/order.wxss */
page,.container{
  min-height: 100%;
  background-color: #f5f5f9;
  color: #333;
  font-size: 26rpx;
}

navigator {
  overflow: hidden;
}
.container {
  width: 100%;
  display: block;
  position: absolute;
}
.container.isAdvert{
  background-image: url(http://maint.duofriend.com/upload/logo_icon.png);
  background-size: 262rpx 39rpx;
  background-position: center 98%;
  background-repeat: no-repeat;
}

.l,.r{
  display: inline-block;
  vertical-align: top;
}
.color01{
  color: #e4393c;
}
.vib{
  visibility: hidden;
}

.header{
  width: 100%;
  height: 155rpx;
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #fff;
}
.header .add image{
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
  margin-right: 16rpx;
}
.header .add text{
  vertical-align: middle;
}
.header .edit {
  width: 100%;
  box-sizing: border-box;
  padding-left: 27rpx;
  line-height: 35rpx;
}
.header .edit .name{
  font-size: 27rpx;
  margin-bottom: 8rpx;
}
.header .edit .address{
  width: 100%;
  font-size: 22rpx;
  color: #87858f;
  position: relative;
}
.header .edit .address>view{
  width: 670rpx;
  display: inline-block;
  vertical-align: top;
}
.header .edit image{
  width: 14rpx;
  height: 21rpx;
  display: inline-block;
  vertical-align: top;
  margin-top: 8rpx;
  margin-left: 10rpx;
}

.bor{
  width: 100%;
  height: 7rpx;
  background-image: url(http://img.liveapp.cn/group3/eng/00/9a/76191c168345b8b2da6c3124a3dc_14930026490722_560813.jpg);
  background-size: 109rpx 7rpx;
  background-position-x: -20rpx;
}



/*********************************/

.shopName {
  width: 100%;
  box-sizing: border-box;
  padding: 16rpx 27rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  border-bottom: 1rpx solid #f5f5f9;
  background-color: #fff;
}

.shopName image {
  width: 47rpx;
  height: 40rpx;
  vertical-align: top;
  margin-right: 15rpx;
}

/*********************************/
.shop-list{
  width: 100%;
  background-color: #fff;
  border-bottom: 1rpx solid #e8e8ed;
  box-sizing: border-box;
  padding: 28rpx 0;
}
.shop-list .l{
  font-size: 0;
}
.shop-list .l image{
  width: 161rpx;
  height: 161rpx;
  margin-right: 15rpx;
  margin-left: 28rpx;
}
.shop-list .r {
  width: 530rpx;
}
.shop-list .r .title{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.shop-list .r .specifications{
  height: 22rpx;
  font-size: 22rpx;
  color: #87858f;
  margin-top: 10rpx;
  margin-bottom: 50rpx;
}
.shop-list .r .price{
  color: #e4393c;
}
.shop-list .r .price text{
  font-size: 17rpx;
}
.shop-list .r .price view{
  float: right;
  color: #87858f;
}
/************************************/
.main-list{
  width: 100%;
  height: 72rpx;
  line-height: 72rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #e8e8ed;
  box-sizing: border-box;
  padding-left: 22rpx;
}
.main-list.noBor{
  border-bottom: 0rpx;
}
.main-list input{
  width: 633rpx;
  display: inline-block;
  vertical-align: middle;
  font-size: 26rpx;
}
.main-list text{
  vertical-align: middle;
}
.main-list.text-r{
  text-align: right;
  padding-right: 22rpx;
}

/*************************************/
.yhq .l{
  width: 122rpx;
}
.yhq .r{
  width: 570rpx;
  text-align: right;
}
.yhq image{
  width: 16rpx;
  height: 25rpx;
  vertical-align: middle;
  margin-left: 7rpx;
}
.switch{
  padding:8px 10px 7px 8px;
  background-color:#fff;
  font-size: 24rpx;
  line-height: 24rpx;
}
.switch .l .t{
  color: #333;
}
.switch .l .b{
  color: #87858f;
  margin-top: 15rpx;
}
.switch .r{
  float: right;
}

/***********************************/
.bottom{
  background-color: #fff;
  line-height: 42rpx;
  padding: 16rpx 20rpx 14rpx 16rpx;
  border-bottom: 1rpx solid #e8e8ed;
}
.bottom .l,.bottom .r{
  width: 50%;
}
.bottom .l{
  text-align: left;
}
.bottom .r{
  text-align: right;
}

.btn{
  width: 100%;
  background-color: #fff;
  text-align: center;
  padding-bottom: 10rpx;
}
.btn>view{
  width: 690rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 30rpx;
  border-radius: 6rpx;
  margin: 0 auto 17rpx auto;
}
.btn>view:nth-child(1){
  background-color: #e4393c;
  color: #fff;
  border:1rpx solid rgba(0,0,0,.2);
}
.btn>view:nth-child(2){
  background-color: #bbebe9;
  color: #0f8783;
  border:1rpx solid #7bd9d6;
}
.btn>view:nth-child(3){
  background-color: #fff;
  color: #e4393c;
  border:1rpx solid #e4393c;
}

/********************************************/
.yhqListLayer .bg{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.4);
}
.yhqListLayer .main{
  width: 100%;
  position: fixed;
  bottom: -550rpx;
  left: 0;
  background-color: #fff;
  transition: bottom .4s;
  z-index: 11;
}
.yhqListLayer .main.show{
  bottom: 0rpx;
}
.yhqListLayer .main .title{
  height: 71rpx;
  line-height: 71rpx;
  text-align: center;
  font-size: 26rpx;
  border-bottom: 1rpx solid #e9e9e9;
}
.yhqListLayer .main .list{
  border-bottom: 1rpx solid #e9e9e9;
  padding-left: 25rpx;
}
.yhqListLayer .main .list:last-child{
  border-bottom: 0rpx;
}
.yhqListLayer .main .list .l{
  width: 85rpx;
  height: 54rpx;
  vertical-align: middle;
  padding-right: 20rpx;
}
.yhqListLayer .main .list view{
  width: 570rpx;
  line-height: 71rpx;
  display: inline-block;
  vertical-align: middle;
}
.yhqListLayer .main .list .r{
  width: 16rpx;
  height: 25rpx;
  vertical-align: middle;
  margin-left: 7rpx;
}

/********************************************/

.fade {
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 100;
  top: 0;
  left: 0;
}

.bindPhone {
  width: 640rpx;
  background-color: #fafafc;
  border-radius: 6rpx;
  z-index: 102;
  position: fixed;
  left: 50%;
  top: 50%;
  margin-left: -320rpx;
  margin-top: -150rpx;
}
.bindPhone .close{
    top:-23rpx;
    right:-23rpx;
}

.bindPhoneTit {
  width:551rpx;
  height:117rpx;
  text-align:center;
  margin:0 auto;
  line-height:117rpx;
  box-sizing:border-box;
  font-size:30rpx;
  color:#010101;
}

.bindPhoneItem {
  width: 532rpx; 
  height: 70rpx;
  line-height: 70rpx;
  font-size: 27rpx;
  margin: 0 auto;
  border: 2rpx solid #e1e1e2;
  border-radius: 5rpx;
}

.bindPhoneItem input {
  width: 500rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: left;
  padding-left: 20rpx;
}
.bindPhoneItem .areaCode{
  display: inline-block;
  width: 50px;
  height:50rpx;
  line-height:50rpx;
  text-align:center;
  vertical-align: 5px;
  border-right: 1px solid #f1f1f1;
   color: #666666;
}
.bindPhoneItem .areaCode view{
  display: inline-block;
}
.bindPhoneItem .areaCode image{
  width:16rpx;
  height:25rpx;
  vertical-align:top;
  transform: rotate(90deg);/* 180°旋转图片。   */
  margin-left: 8px;
  margin-top: 8px;

}
.bindPhoneItem .phoneNumber {
  width: 360rpx;
  display: inline-block;
}

.bindPhoneItem .phoneCode {
  width: 309rpx;
  display: inline-block;
  vertical-align: top;
}

.bindPhoneItem .sendCodeBtn {
  width:200rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  display: inline-block;
  font-size: 27rpx;
  color: #1deb1d;
  vertical-align: top;
}

.bindPhoneItem .sendCodeBtn.disabled {
  color: #ccc;
}

.bindsubmitBox {
  width: 100%;
  height: 83rpx;
  line-height: 83rpx;
  text-align: center;  
  border-top: 1rpx solid #d5d5d6;
}

.bindsubmit {
  font-size: 31.24rpx;
  color: #1deb1d;
}

/*.bindsubmit.disabled {
  color: #ccc;
}*/

.other {
  padding: 10rpx;
  border: 1px solid #e5e5e5;
  margin: 20rpx;
  border-radius: 5rpx;
}

.other input {
  width: 100%;
  height: 50rpx;
  font-size: 25rpx;
}
